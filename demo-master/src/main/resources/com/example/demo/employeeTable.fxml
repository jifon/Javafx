<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="0.0"
            minWidth="0.0"
            prefHeight="450.0"
            prefWidth="700.0"
            xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/17"
            fx:controller="com.example.demo.Controller.Employee.EmployeeTableController">
   <children>
      <TableView fx:id="employeeTable"  prefHeight="368.0" prefWidth="700.0">
         <columns>
            <TableColumn fx:id="fNameColumn" prefWidth="100.0" text="Имя" />
            <TableColumn fx:id="lNameColumn" prefWidth="100.0" text="Фамилия" />
            <TableColumn fx:id="ssnColumn" prefWidth="100.0" text="Ssn" />
            <TableColumn fx:id="bdateColumn" prefWidth="100.0" text="Дата рождения" />
            <TableColumn fx:id="addressColumn" prefWidth="100.0" text="Адрес" />
            <TableColumn fx:id="sexColumn" prefWidth="50.0" text="Пол" />
            <TableColumn fx:id="editColumn" prefWidth="75.0" text="Изменение" />
            <TableColumn fx:id="deleteColumn" prefWidth="75.0" resizable="false" text="Удаление" />
         </columns>
      </TableView>
      <Button fx:id="addButton" onAction="#openCreatePage" defaultButton="true" layoutX="200.0" layoutY="395.0" mnemonicParsing="false" prefHeight="8.0" prefWidth="300.0" text="Добавить новую запись" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
